{
    "name": "laasti/application",
    "description": "The core for the Laasti Frameworks. Provides interfaces to implement essential services",
    "autoload": {
        "psr-4": {
            "Laasti\\": "src/"
        }
    },
    "require": {
        "league/container": "~1.3",
        "symfony/http-foundation": "~2.6",
        "league/route": "master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "laasti/stack": "master",
        "laasti/profiler": "master",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nebulousGirl/route"
        },
        {
            "type": "vcs",
            "url": "C:/wamp/www/laasti/stack"
        },
        {
            "type": "vcs",
            "url": "C:/wamp/www/laasti/profiler"
        }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sonia Marquette",
            "email": "contact@nebulousweb.com"
        }
    ],
    "minimum-stability": "dev"
}
